<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:b='urn:import:org.gwtbootstrap3.client.ui'>
	<g:HTMLPanel>



		<b:Container ui:field="c">

			<b:Row>
				<b:Column size="XS_12">

					<b:PageHeader text="Spring Boot GWT BootStrap3" />

					<b:Panel>
						<b:PanelHeader>
							<b:Heading size="H3" text="Sign On" />
						</b:PanelHeader>
						<b:PanelBody>
							<b:Jumbotron>
							<b:Form>
								<b:FormLabel ui:field="login1">Login</b:FormLabel>
								<b:TextBox ui:field="login" />
								<b:FormLabel ui:field="pwd1">Password</b:FormLabel> 
								<b:Input type="PASSWORD" ui:field="password"/>
								<b:SubmitButton ui:field="submit" text="Login" />
							</b:Form>	
							</b:Jumbotron>
						</b:PanelBody>
						<b:PanelFooter>
									<b:Heading size="H6" text="iQmSoft" />
						</b:PanelFooter>
					</b:Panel>
				</b:Column>

			</b:Row>



		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder>
